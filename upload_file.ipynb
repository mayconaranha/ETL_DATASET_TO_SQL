    "if not df.empty:\n",
    "    try:\n",
    "        df.to_sql(table_name, con=engine, if_exists='append', index=False)\n",
    "        print(f\"Dados inseridos com sucesso na tabela {table_name}.\")\n",
    "    except Exception as e:\n",
    "        print(f\"Erro ao inserir dados: {e}\")\n",
